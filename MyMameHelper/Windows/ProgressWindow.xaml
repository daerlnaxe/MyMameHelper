<Window x:Class="MyMameHelper.Windows.ProgressWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MyMameHelper.Windows"
        mc:Ignorable="d"
        Title="Progress" Height="450" Width="800" Loaded="Window_Loaded">
    <Grid>
        <ProgressBar x:Name="progressB" HorizontalAlignment="Left" Height="101" Margin="76,234,0,0" VerticalAlignment="Top" Width="678" Value="{Binding Progress_Value, IsAsync=True}" Maximum="{Binding Total}"/>
        <Label x:Name="label" Content="{Binding Progress_Value, IsAsync=True}" HorizontalAlignment="Left" Height="46" Margin="98,140,0,0" VerticalAlignment="Top" Width="71"/>
     

    </Grid>
</Window>
